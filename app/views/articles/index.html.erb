<div class="contents row">
  <%= link_to image_tag("adventure.png", class: "head_img"), "#" %>
  <h1>タイムライン</h1>
  <% @articles.each do |article| %>
    <div class="content_post">
      <ul class="article_overview">
        <li><%= article.title %></li>
        <li><%= article.activity_date %></li>
        <li class="user_name"><%= article.user.name %></li>
        <li><%#= article.major_id.name %></li>
        <li class="icon_image"><%#= image_tag @users.image, class: 'message-image' %></div></li>
      </ul>
    </div>
  <% end %>
</div>